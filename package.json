{
  "name": "@devoxa/next-i18next-compress",
  "description": "Automatically compress locale keys for `next-i18next`",
  "version": "1.2.0",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/devoxa/next-i18next-compress"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier --ignore-path='.gitignore' --list-different --write .",
    "format:check": "prettier --ignore-path='.gitignore' --check .",
    "lint": "eslint --ignore-path='.gitignore' '{src,tests}/**/*.ts'",
    "build": "rm -rf dist/ && tsc",
    "preversion": "yarn build"
  },
  "eslintConfig": {
    "extends": "@devoxa"
  },
  "prettier": "@devoxa/prettier-config",
  "dependencies": {
    "colors": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/traverse": "^7.14.2",
    "@babel/types": "^7.14.2",
    "@devoxa/eslint-config": "^2.0.5",
    "@devoxa/prettier-config": "^1.0.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^15.0.1",
    "eslint": "^7.0.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
