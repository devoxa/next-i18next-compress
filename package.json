{
  "name": "@devoxa/next-i18next-compress",
  "description": "Automatically compress locale keys for `next-i18next`",
  "version": "1.3.1",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/devoxa/next-i18next-compress"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier --ignore-path='.prettierignore' --list-different --write .",
    "format:check": "prettier --ignore-path='.prettierignore' --check .",
    "lint": "eslint --ignore-path='.prettierignore' '{src,tests}/**/*.ts'",
    "build": "rm -rf dist/ && tsc",
    "preversion": "yarn build"
  },
  "eslintConfig": {
    "extends": "@devoxa"
  },
  "prettier": "@devoxa/prettier-config",
  "dependencies": {
    "colors": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@babel/traverse": "7.18.8",
    "@babel/types": "7.18.8",
    "@devoxa/eslint-config": "3.0.1",
    "@devoxa/prettier-config": "1.0.2",
    "@types/jest": "28.1.5",
    "@types/node": "16.11.44",
    "eslint": "8.19.0",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
