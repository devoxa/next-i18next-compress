{
  "name": "@devoxa/next-i18next-compress",
  "description": "Automatically compress locale keys for `next-i18next`",
  "version": "1.3.1",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/devoxa/next-i18next-compress"
  },
  "scripts": {
    "test": "jest",
    "format": "prettier --ignore-path='.prettierignore' --list-different --write .",
    "format:check": "prettier --ignore-path='.prettierignore' --check .",
    "lint": "eslint --ignore-path='.prettierignore' '{src,tests}/**/*.ts'",
    "build": "rm -rf dist/ && tsc",
    "preversion": "yarn build"
  },
  "eslintConfig": {
    "extends": "@devoxa"
  },
  "prettier": "@devoxa/prettier-config",
  "dependencies": {
    "colors": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-syntax-jsx": "7.14.5",
    "@babel/traverse": "7.14.7",
    "@babel/types": "7.14.5",
    "@devoxa/eslint-config": "2.0.5",
    "@devoxa/prettier-config": "1.0.0",
    "@types/jest": "26.0.23",
    "@types/node": "15.12.5",
    "eslint": "7.30.0",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
